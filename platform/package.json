{
  "name": "@doubledice/platform",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "clean": "rimraf abi artifacts build cache generated lib/generated subgraph/generated",
    "test": "hardhat typechain && hardhat test",
    "coverage": "hardhat coverage",
    "testcoins:deploy:local": "   hardhat run ./scripts/deploy-test-coins.ts  --network localhost",
    "testcoins:deploy:polygon": " hardhat run ./scripts/deploy-test-coins.ts  --network polygon",
    "tokens:whitelist:polygon": " hardhat run ./scripts/whitelist-tokens.ts   --network polygon",
    "contracts:deploy:local": "   hardhat run ./scripts/deploy.ts  --network localhost",
    "contracts:deploy:docker": "  hardhat run ./scripts/deploy.ts  --network docker",
    "contracts:deploy:rinkeby": " hardhat run ./scripts/deploy.ts  --network rinkeby",
    "contracts:deploy:mumbai": "  hardhat run ./scripts/deploy.ts  --network mumbai",
    "contracts:deploy:polygon": " hardhat run ./scripts/deploy.ts  --network polygon",
    "contracts:upgrade:local": "  hardhat run ./scripts/upgrade.ts --network localhost",
    "contracts:upgrade:docker": " hardhat run ./scripts/upgrade.ts --network docker",
    "contracts:upgrade:rinkeby": "hardhat run ./scripts/upgrade.ts --network rinkeby",
    "contracts:upgrade:mumbai": " hardhat run ./scripts/upgrade.ts --network mumbai",
    "graph:codegen": "./scripts/graph-codegen.sh",
    "graph:build": "npm run graph:codegen && graph build ./subgraph/generated/subgraph.yaml",
    "graph:create:docker": "graph create local/local --node http://graph-node:8020",
    "graph:deploy:local:local": "                             npm run graph:build && graph deploy local/local      --ipfs http://ipfs:5001 --node http://graph-node:8020           --version-label v1 ./subgraph/generated/subgraph.yaml",
    "graph:deploy:mumbai-dev": "                              npm run graph:build && graph deploy doubledicedev/doubledice-mumbai          --node https://api.thegraph.com/deploy/ --version-label v1 ./subgraph/generated/subgraph.yaml",
    "graph:deploy:mumbai-beta": "                             npm run graph:build && graph deploy doubledicedev/doubledice-mumbai2         --node https://api.thegraph.com/deploy/ --version-label v1 ./subgraph/generated/subgraph.yaml",
    "graph:deploy:doubledicedev:doubledice-polygon-mainnet": "npm run graph:build && graph deploy doubledicedev/doubledice-polygon-mainnet --node https://api.thegraph.com/deploy/ --version-label v1 ./subgraph/generated/subgraph.yaml",
    "graph:deploy:ddvfs-com:ddvfs-polygon": "                 npm run graph:build && graph deploy ddvfs-com/ddvfs-polygon                  --node https://api.thegraph.com/deploy/ --version-label v1 ./subgraph/generated/subgraph.yaml",
    "graph:all:docker": "npm run graph:build && npm run graph:create:docker && npm run graph:deploy:local:local",
    "test:local:create-vf": "hardhat run --network localhost ./scripts/create-test-vf.ts",
    "codegen": "hardhat compile && npm run graph:codegen",
    "postinstall": "npm run codegen",
    "pregen-sol-interface": "npx hardhat compile",
    "gen-sol-interface": "cat ./generated/abi/DoubleDice.json | abi-to-sol --validate --solidity-version=0.8.12 IDoubleDiceComplete > ./generated/IDoubleDiceComplete.sol",
    "lint:sol": "solhint contracts/*.sol contracts/*/*.sol contracts/*/*/*.sol"
  },
  "main": "./lib/index.ts",
  "files": [
    "lib/*"
  ],
  "devDependencies": {
    "@assemblyscript/loader": "0.20.6",
    "@graphprotocol/graph-cli": "0.30.0",
    "@graphprotocol/graph-ts": "0.27.0",
    "@graphql-codegen/cli": "2.6.2",
    "@graphql-codegen/typescript": "2.4.11",
    "@nomiclabs/hardhat-ethers": "2.0.6",
    "@nomiclabs/hardhat-etherscan": "3.0.3",
    "@nomiclabs/hardhat-waffle": "2.0.3",
    "@opengsn/contracts": "2.2.4",
    "@openzeppelin/contracts": "^4.8.0",
    "@openzeppelin/contracts-upgradeable": "4.5.2",
    "@typechain/ethers-v5": "10.0.0",
    "@typechain/hardhat": "6.0.0",
    "@types/chai": "4.3.1",
    "@types/chai-subset": "1.3.3",
    "@types/expect": "24.3.0",
    "@types/mocha": "9.1.1",
    "@types/node": "16.11.36",
    "@typescript-eslint/eslint-plugin": "5.25.0",
    "@typescript-eslint/parser": "5.25.0",
    "abi-to-sol": "0.5.3",
    "ajv": "8.11.0",
    "ajv-formats": "2.1.1",
    "assemblyscript": "0.20.6",
    "axios": "0.27.2",
    "bignumber.js": "9.0.2",
    "chai": "4.3.6",
    "chai-subset": "1.6.0",
    "dotenv": "16.0.1",
    "envsub": "4.0.7",
    "eslint": "8.16.0",
    "ethereum-waffle": "3.4.4",
    "ethers": "5.6.7",
    "ethlint": "1.2.5",
    "hardhat": "2.9.6",
    "hardhat-abi-exporter": "2.9.0",
    "hardhat-gas-reporter": "1.0.8",
    "mocha": "10.0.0",
    "rimraf": "3.0.2",
    "solhint": "3.3.7",
    "solidity-coverage": "0.7.21",
    "ts-mocha": "10.0.0",
    "ts-node": "10.8.0",
    "typechain": "8.0.0",
    "typescript": "4.6.4"
  },
  "dependencies": {
    "@gelatonetwork/relay-context": "^2.1.0",
    "@openzeppelin/contracts-ethereum-package": "^3.0.0",
    "@openzeppelin/gsn-helpers": "^0.2.4",
    "@openzeppelin/network": "^0.4.3",
    "@openzeppelin/upgrades": "^2.8.0"
  }
}
